<swiper class="swiper" circular autoplay interval="3000" duration="300" 
                        indicator-dots indicator-active-color='#ff5777' style="height:390rpx"
                       >
        <block wx:for="{{list}}" wx:key="index">
          <swiper-item>
            <view class="swiper-item">
              <image src="{{item.image}}" mode="widthFix" style="width:100%;"/>
            </view>
          </swiper-item>
        </block>
</swiper>